<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="eklerni.form.type.login" class="Eklerni\CASBundle\Form\Type\LoginType">
            <tag name="form.type" alias="eklerni_login"/>
        </service>
        <service id="eklerni.form.type.profile.changepassword" class="Eklerni\CASBundle\Form\Type\ChangePasswordType">
            <tag name="form.type" alias="eklerni_password_change"/>
        </service>
        <service id="eklerni.form.type.profile.update" class="Eklerni\CASBundle\Form\Type\ProfileType">
            <tag name="form.type" alias="eklerni_profile_update"/>
        </service>
    </services>

</container>