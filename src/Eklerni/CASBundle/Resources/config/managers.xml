<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="manager_ecole.class">Eklerni\CASBundle\EcoleManager</parameter>
        <parameter key="manager_classe.class">Eklerni\CASBundle\ClasseManager</parameter>
        <parameter key="manager_eleve.class">Eklerni\CASBundle\EleveManager</parameter>
        <parameter key="manager_enseignant.class">Eklerni\CASBundle\EnseignantManager</parameter>
        <parameter key="manager_matiere.class">Eklerni\CASBundle\MatiereManager</parameter>
        <parameter key="manager_activite.class">Eklerni\CASBundle\ActiviteManager</parameter>
        <parameter key="manager_serie.class">Eklerni\CASBundle\SerieManager</parameter>

    </parameters>

    <services>
        <service id="eklerni.manager_ecole" class="%manager_ecole.class%"/>
        <service id="eklerni.manager_classe" class="%manager_classe.class%"/>
        <service id="eklerni.manager_eleve" class="%manager_eleve.class%"/>
        <service id="eklerni.manager_enseignant" class="%manager_enseignant.class%"/>
        <service id="eklerni.manager_matiere" class="%manager_matiere.class%"/>
        <service id="eklerni.manager_activite" class="%manager_activite.class%"/>
        <service id="eklerni.manager_serie" class="%manager_serie.class%"/>

    </services>
</container>