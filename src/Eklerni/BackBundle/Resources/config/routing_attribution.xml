<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="eklerni_back_attribution" pattern="/">
        <default key="_controller">EklerniBackBundle:Attribution:index</default>
    </route>
    <route id="eklerni_back_attribution_ajax_addSerieEleve" pattern="/addSerieEleve">
        <default key="_controller">EklerniBackBundle:Attribution:addSerieEleve</default>
    </route>
    <route id="eklerni_back_attribution_ajax_addSerieClasse" pattern="/addSerieClasse">
        <default key="_controller">EklerniBackBundle:Attribution:addSerieClasse</default>
    </route>
    <route id="eklerni_back_attribution_ajax_activerAttribution" pattern="/activerAttribution">
        <default key="_controller">EklerniBackBundle:Attribution:activerAttribution</default>
    </route>


</routes>